{
  "name": "ngo-nabarun-fe",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:dev": "ng serve --host 0.0.0.0 --disable-host-check",
    "start:stage": "ng serve --configuration stage --port 5201",
    "build": "ng build",
    "build:fast": "ng build --configuration development",
    "build:stage": "ng build --configuration stage",
    "watch": "ng build --watch --configuration development",
    "serve:fast": "ng serve --configuration development --disable-host-check --poll=2000",
    "sync:api": "ng-openapi-gen --input https://ngonabarun-stage.appspot.com/v3/api-docs --output src/app/core/api",
    "sync:api2": "ng-openapi-gen --input http://localhost:8080/api/docs --output src/app/core/api-client"
  },
  "private": true,
